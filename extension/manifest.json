{
    "name" : "나무위키 문서추천",
    "version" : "0.1",
    "description" : "Namu wiki recommendation",
    "permissions" : ["storage"],
    "background" : {
        "scripts" : ["src/jquery-3.3.1.js", "src/main.js"],
        "persistent" : false
    },
    "host_permissions" : [
        "*://namu.wiki/*"
    ],
    "action": {
        "default_icon": "icon/icon.png",
        "default_popup": "src/popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://namu.wiki/*"
            ],
            "js": [
                "src/jquery-3.3.1.js",
                "src/main.js"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "manifest_version" : 3
}